{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "resolveJsonModule":true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "rootDir": "./src",
        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny":true,
        "strictNullChecks":false
    },
    "exclude": [
      "node_modules",
   ]
  }